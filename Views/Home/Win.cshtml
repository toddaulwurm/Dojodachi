@{
    ViewData["Title"] = "Home Page";
}


<div class="text-center">


    <h1 class="display-4">MOCHI IS SO FULL, BUZZED AND HAPPY!!! CONGRATS!! :)</h1>
    <img style="height: 70px" src="data:image/png;base64," alt="Happy Mochi">
    <h2>Message: @ViewBag.Message</h2>
    <p>Happiness: @ViewBag.Happiness</p>
    <p>Fullness: @ViewBag.Fullness</p>
    <p>Energy: @ViewBag.Energy</p>
    <p>Meals: @ViewBag.Meals</p>
</div>
<hr>
<br>
    <form asp-action="reset" method="post" asp-controller="Home">
        <input type="submit" class="btn btn-secondary btn-sm mt-2" value="Start Again? :)">
    </form>